<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:design="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_man_hinh_chinh"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.greenacademy.travelapp.Activity.Activity.ManHinhChinh.ManHinhChinhActivity">


    <LinearLayout
        android:id="@+id/menu"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:orientation="horizontal">

        <include
            android:id="@+id/Menu_DiaDiem"
            layout="@layout/manhinhchinh_menu_diadiem" />

        <include
            android:id="@+id/Menu_QuanAn"
            layout="@layout/manhinhchinh_menu_quanan" />

        <include
            android:id="@+id/Menu_HanhTrinh"
            layout="@layout/manhinhchinh_menu_hanhtrinh" />

        <include
            android:id="@+id/Menu_BanThan"
            layout="@layout/manhinhchinh_menu_nguoidung" />

    </LinearLayout>

    <FrameLayout
        android:id="@+id/framlayout_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
